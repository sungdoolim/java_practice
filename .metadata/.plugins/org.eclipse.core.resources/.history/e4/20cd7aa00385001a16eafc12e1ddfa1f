package week6;

import java.util.Scanner;

public class no1 {
	node head=new node();
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
	
		String s;
		while(true) {
		s=sc.next();
		btree(s);
		}
	}
	static void btree(String s) {
		s.charAt(0);
		switch(s.charAt(0)) {
		case '+':
			s=s.substring(1, s.length());
			System.out.println(s);
			break;
			
			
			
		case '-':
			break;
			
			
			
		case '?':
			break;
			
		}
	}

}
