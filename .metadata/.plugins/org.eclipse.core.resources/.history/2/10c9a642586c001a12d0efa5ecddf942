//2015112120 임성두
package week1;
import java.util.Scanner;
public class no2_5 {

	static int []replyM=new int[100];

	int d;
	
	public static void main(String[] args) {

		Scanner sc=new Scanner(System.in);
		 d=sc.nextInt();// 코인 배열 크기를 먼저 입력받습니다.
		int []c=new int[d];
		
		for(int i=0;i<c.length;i++) {
				c[i]=sc.nextInt();// 코인의 종류를 입력 받습니다.
			}
		int different=0;
		for(int M=0;M<100;M++) {
		int GreedyResult=GreedyBetterChange(M,c,d);
		int BruteFoceResult=(int)bruteforce(M,c, d);
		replyM[M]=BruteFoceResult;
		
		if(GreedyResult!=BruteFoceResult) {
			different++;
		System.out.print(M+": "+GreedyResult+", ");
		System.out.println(BruteFoceResult);
		
		}
	  }System.out.println("틀린 갯수 : "+different);
		
	}
	
	public static int GreedyBetterChange(int M,int []c,int d) {
		if(M==0)
			return 0;

		for(int i=0;i<d;i++) {
			if(M>=c[i]) {
				return GreedyBetterChange(M-c[i], c, d)+1;
			}
		}
		return 1;

	}
		

		
		public static int bruteforce(int M,int[]c,int d) {
			
			if(M==0) {
				return 0;
			}else if(replyM[(int) M]!=0) {
				return (int)replyM[(int) M];
			}

			
			
			for(int i=0;i<d;i++) {
				if(M>=c[i])
				result[i]= bruteforce(M-c[i],c,d)+1;
			}
	return result;
		}

		
	
	
}
