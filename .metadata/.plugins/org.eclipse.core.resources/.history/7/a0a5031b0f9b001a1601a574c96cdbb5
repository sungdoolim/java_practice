	static String Spectrum(String []s,int l) {
		String t="";
		String t1="";
		for(int i=0;i<l-1;i++) {
			t=s[i].substring(1);
			t1=s[i+1].substring(0,l-1);
			if(t.equals(t1)) {
				
			}
		}
		
		return "";
	}

