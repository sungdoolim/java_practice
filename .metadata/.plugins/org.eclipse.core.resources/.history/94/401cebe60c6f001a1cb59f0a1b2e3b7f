package week2;

import java.util.Scanner;

public class no2_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc=new Scanner(System.in);
		int a,b;
		a=sc.nextInt();b=sc.nextInt();
		System.out.println(gcd(a,b));
		
	}
	static int gcd(int a, int b) {
	
		int t=0;
		while(a=!=0) {
			if(a>b) {
				t=a%b;
				a=t;
			}
		}
		
		
		
		return 1;
		
	}

}
