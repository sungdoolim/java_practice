package week5;

public class no2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int M=11;
		int Mar[]=new int[M];
		int c1=1,c2=4,c3=7;
		coinDynamic(Mar,M,c1,c2,c3);

	}
	static void coinDynamic(int Mar[],int M,int c1,int c2,int c3) {
		int t1,t2,t3;
		for(int i=0;i<M;i++) {
			t1=Mar[i-c1]+1;
			t2=Mar[i-c2]+1;
			t3=Mar[i-c3]+1;
			
			
			System.out.print(Mar[i]+" ");
			
		}
	}

}
