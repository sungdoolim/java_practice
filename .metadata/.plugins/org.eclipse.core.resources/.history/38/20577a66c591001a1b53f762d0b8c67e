import java.util.ArrayList;

public class prac2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
String a[]= {"DIA", "RUBY", "RUBY", "DIA", "DIA", "EMERALD", "SAPPHIRE", "DIA"};
		solution(a);
	
		
	}//   \\+ \\* -

    public static int[] solution(String[] gems) {
        
        int gl=gems.length;
        //String[]gem = new String[gl];
        ArrayList<String>gem=new ArrayList<>();
       gem.add(gems[0]);
       
     //   String []
        int count=1;
        for(int i=0;i<gl;i++) {
        for(int j=0;j<gem.size();j++) {
        	
        	//System.out.println(gem.size());
        	if(gem.contains(gems[i])) {
        		break;
        	}else {
        		count++;
        		gem.add(gems[i]);
        		continue;
        	}
        }
        	
        	
        	
        }
        int c1=1;
        int []a=new int[gl];
        int []at=new int[gl];
        ArrayList<String>gemt=new ArrayList<>();
        System.out.println(count);
        
        
        
        for(int i=0;i<gl;i++) {
        	c1=1;
        	gemt.clear();
        	gemt.add(gems[i]);
        	for(int j=i;j<gl;j++) {
        		if(!(gemt.contains(gems[j]))) {
        			c1++;
        			gemt.add(gems[j]);
        		}
        		if(c1==count) {
        			a[i]=j;
        		}
        		
        	}
        	
        	
        	
        }
        
int min=0;
int c=0;
    	for(int j=0;j<gl;j++) {
    	System.out.print(a[j]);
    		 at[j]=a[j]-j;
    		 System.out.println(at[j]);
    	}
    	
    	 min=at[0];
    	 c=0;
    	for(int j=1;j<gl;j++) {
    		if(at[j]<=0)
    			at[j]=100000;
    		
    		if(min>at[j]) {
    			min=at[j];
    			c=j;}
    					
    	}
    	System.out.println(c);
    	int[] answer = {c,min};
        return answer;
    }
}
