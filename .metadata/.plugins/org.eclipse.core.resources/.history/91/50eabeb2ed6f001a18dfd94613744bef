package week2;

import java.util.Scanner;

public class no2_2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int a,b;
		a=sc.nextInt();b=sc.nextInt();
		System.out.println(gcd(a,b));
	}
	static int gcd(int a, int b) {
		
		if(a!=0&&b==0) {
			return a;
		}else if(a==0&&b!=0) {
			return b;
		}
		else if(a>b) {
			return gcd(b,a%b);
		}else {
			return gcd(a,b%a);
		}
	
		
	}
}
