package javapractice;

import java.util.Scanner;

public class 소풍 {


	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int testcase=sc.nextInt();
	int student=0;
	int couple=0;
	int len=0;
	int []ar;
	
	for(int i=0;i<testcase;i++) {
		
		ar=null;
		student=sc.nextInt();
		couple=sc.nextInt();
		len=2*couple;
		ar=new int[couple];
		for(int j=0;j<2;j++) {
			ar[j]=new int[2];
		for(int j=0;j<couple;j++) {
		ar[j][i]=sc.nextInt();	
		}
		}
		
		
		}
	
	
	}
}
